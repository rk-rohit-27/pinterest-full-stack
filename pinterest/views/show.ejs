<%- include('./partials/header') -%>

    <div class="w-full min-h-screen  ">

        <div class="cards ml-10 mt-10 flex flex-wrap gap-10">
            <% user.posts.forEach(function (elem) { %>
                <a href="/show/posts/<%= elem._id %>">
                    <div class="card w-32">
                        <div class=" w-full h-40  rounded-lg overflow-hidden">
                            <img class="w-full h-full object-cover" src="/images/uploads/<%= elem.image %>" alt="">
                        </div>
                        <h5 class="mt-2 text-sm">
                            <%= elem.title %>
                        </h5>
                    </div>
                    <% }) %>

        </div>
        </a>
    </div>

    <%- include('./partials/footer') -%>